<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.fhi.kidmap.business.Training" table="TRAINING">
    <id column="RECORDID" name="recordId" type="string"/>
    <property column="CATEGORY" name="category" type="string"/>
    <property column="DESIGNATION" name="designation" type="string"/>
    <property column="ENDDATE" name="endDate" type="string"/>
    <property column="STARTDATE" name="startDate" type="string"/>
    <property column="PARTICIPANTID" name="participantId" type="string"/>
    <property column="TRAININGID" name="trainingId" type="string"/>
    <property column="TRAININGSTATUS" name="trainingStatus" type="string"/>
    <property column="DATEMODIFIED" name="dateModified" type="string"/>
    <property column="PARTNERCODE" name="partnerCode" type="string"/>
  </class>
</hibernate-mapping>
