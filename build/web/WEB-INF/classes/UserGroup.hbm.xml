<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.fhi.kidmap.business.UserGroup" table="USERGROUP">
     <id column="GROUPID" name="groupId" type="string"/>
    <property column="GROUPNAME" name="groupName" type="string"/>
    <property column="GROUPTYPE" name="groupType" type="string"/>
    <property column="ORGUNITGROUP" name="orgUnitGroup" type="string"/>
    <property column="DATECREATED" name="dateCreated" type="string"/>
    <property column="DATEMODIFIED" name="dateModified" type="string"/>
    <property column="DESCRIPTION" name="description" type="string"/> 
  </class>
</hibernate-mapping>
