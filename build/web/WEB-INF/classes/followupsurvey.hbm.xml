<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class lazy="false" name="com.fhi.kidmap.business.FollowupSurvey" table="survey">
    <id column="id" name="id" type="integer">
      <generator class="identity"/>
    </id>
    <property column="ovc_Id" name="ovcId" type="string"/>
    <property column="date_of_survey" name="dateOfSurvey" type="string"/>
    <property column="address" name="updatedAddress" type="string"/>
    <property column="age" name="updatedAge" type="int"/>
    <property column="ageunit" name="updatedAgeUnit" type="string"/>
    <property column="hiv_status" name="updatedHivStatus" type="string"/>
    <property column="birth_certificate" name="updatedBirthCertStatus" type="string"/>
    <property column="school_status" name="updatedSchoolStatus" type="string"/>
    <property column="school_name" name="updatedSchoolName" type="string"/>
    <property column="UPDATEDCLASS" name="updatedClass" type="string"/>
    <property column="CAREGIVERID" name="caregiverId" type="string"/>
    <property column="CGIVERHIVSTATUS" name="cgiverHivStatus" type="string"/>
    <property column="VCENROLLEDINCARE" name="enrolledInCare" type="string"/>
    <property column="VCENROLLEDONART" name="enrolledOnART" type="string"/>
    <property column="VCFACILITYID" name="facilityId" type="string"/>
    <property column="CGIVERENROLLEDINCARE" name="cgiverEnrolledInCare" type="string"/>
    <property column="CGIVERENROLLEDONART" name="cgiverEnrolledOnART" type="string"/>
    <property column="CGIVERFACILITYID" name="cgiverFacilityId" type="string"/>
    <property column="caregiver_relationship" name="updatedCaregiverRelationship" type="string"/>
    <property column="completedby_name" name="completedbyName" type="string"/>
    <property column="completedby_designation" name="completedbyDesignation" type="string"/>
    <property column="recordedby" name="recordedBy" type="string"/>
    <property column="dateofentry" name="dateOfEntry" type="string"/>
  </class>
</hibernate-mapping>
