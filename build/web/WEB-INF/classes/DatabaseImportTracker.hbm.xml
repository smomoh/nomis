<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.fhi.nomis.nomisutils.DatabaseImportTracker" table="DATABASEIMPORTTRACKER">
    <id column="RECORDID" name="recordId" type="int">
      <generator class="identity"/>
    </id>
    <property column="DATEOFIMPORT" name="dateOfImport" type="string"/>
    <property column="SUMMARY" name="summary" type="string"/>
    <property column="USERNAME" name="userName" type="string"/>
    <property column="DATEANDTIME" name="dateAndTime" type="string"/>
    <property column="XMLFILENAME" name="fileName" type="string"/>
    <property column="RESPONSESENT" name="responseSent" type="string"/>
  </class>
</hibernate-mapping>
