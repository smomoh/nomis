<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class lazy="false" name="com.fhi.kidmap.business.Ward" table="ward">
    <id column="ward_code" name="wardCode" type="string">
      <!--<generator class="increment"/>-->
    </id>
    <property column="ward_name" name="wardName" type="string"/>
    <property column="lga_code" name="lgaCode" type="string"/>
    <property column="state_code" name="stateCode" type="string"/>
    <property column="cbo_code" name="cboCode" type="string"/>
  </class>
</hibernate-mapping>
