<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.fhi.kidmap.business.SchoolAttendance" table="SCHOOLATTENDANCE">
      <id name="recordId" column="RECORDID" type="integer">
        <generator class="identity" />
      </id>
      <property name="caregiverId" column="CAREGIVERID" type="string"/>
      <property name="childMissSchool" column="CHILDMISSSCHOOL" type="string" />
      <property name="dateOfAssessment" column="DATEOFASSESSMENT" type="date" />
      <property name="lastModifiedDate" column="LASTMODIFIEDDATE" type="date" />
      <property name="ovcId" column="OVCID" type="string" />
      <property name="reasonsForMissingSchool" column="REASONSFORMISSINGSCHOOL" type="string"/>
      <property name="recordedBy" column="RECORDEDBY" type="string"/>
  </class>
</hibernate-mapping>
