<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.fhi.kidmap.business.DeletedRecord" table="DELETEDRECORD">
    <id name="id" column="ID" type="integer">
        <generator class="identity" />
    </id>
    <property name="recordId" column="RECORDID" type="string" />
    <property name="dateRecordCreated" column="DATERECORDCREATED" type="string" />
    <property name="dateRecordDeleted" column="DATERECORDDELETED" type="string" />
    <property name="typeOfRecord" column="TYPEOFRECORD" type="string" />
    <property name="newUniqueId" column="NEWUNIQUEID" type="string" />
  </class>
</hibernate-mapping>
