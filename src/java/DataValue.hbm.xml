<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.nomis.business.Datavalue" table="DATAVALUE">
    <id column="RECORDID" name="recordId" type="int">
      <generator class="identity"/>
    </id>
    <property column="CATEGORYCOMBOID" name="categoryComboId" type="string"/>
    <property column="INDICATORID" name="indicatorid" type="string"/>
    <property column="ORGUNITID" name="orgunitid" type="string"/>
    <property column="STARTDATE" name="startdate" type="string"/>
    <property column="VALUE" name="value" type="int"/>
  </class>
</hibernate-mapping>
