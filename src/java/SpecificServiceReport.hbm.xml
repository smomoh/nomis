<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.fhi.kidmap.report.SpecificServiceReport" table="SPECIFICSERVICEREPORT">
    <id column="id" name="id" type="int">
      <generator class="identity"/>
    </id>
        <property column="ovc_id" name="ovcId" type="string"/>
        <property column="servicedate" name="servicedate" type="string"/>
        <property column="date_entry" name="dateOfEntry" type="string"/>
        <property column="PSYCHOSOCIALSUPPORT" name="serviceAccessed1" type="string"/>
        <property column="NUTRITIONALSERVICES" name="serviceAccessed2" type="string"/>
        <property column="HEALTHSERVICES" name="serviceAccessed3" type="string"/>
        <property column="EDUCATIONALSERVICES" name="serviceAccessed4" type="string"/>
        <property column="PROTECTIONSERVICES" name="serviceAccessed5" type="string"/>
        <property column="SHELTERSERVICES" name="serviceAccessed6" type="string"/>
        <property column="ECONOMICSERVICES" name="serviceAccessed7" type="string"/>
        <property column="servicesreferred" name="servicesReferred" type="string"/>
        <property column="weight" name="weight" type="double"/>
        <property column="height" name="height" type="double"/>
        <property column="survey_number" name="surveyNumber" type="int"/>
        <property column="NO_OF_SERVICES" name="numberOfServices" type="int"/>
        <property column="serviceprovider" name="providerName" type="string"/>  
  </class>
</hibernate-mapping>
