<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.fhi.kidmap.business.ProcessMonitor" table="PROCESSMONITOR">
      <id name="processId" column="PROCESSID" type="string" />
      <property name="processName" column="PROCESSSNAME" type="string"/>
      <property name="lastModifiedDate" column="LASTMODIFIED" type="date"/>
      <property name="autogenerated" column="AUTOGENERATED" type="int"/>
      <property name="processStatus" column="PROCESSSTATUS" type="int"/>
      <property name="repeatAction" column="REPEATACTION" type="int"/>
  </class>
</hibernate-mapping>
