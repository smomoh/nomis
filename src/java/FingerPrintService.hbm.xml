<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.nomis.business.FingerPrintService" table="FINGERPRINTSERVICE">
    <id column="id" name="id" type="int">
      <generator class="identity"/>
    </id>
    <property column="MACHINEID" name="machineId" type="string"/>
    <property column="OVC_ID" name="ovcId" type="string"/>
    <property column="SERVICEDATE" name="servicedate" type="string"/>
  </class>
</hibernate-mapping>
