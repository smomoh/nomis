<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.nomis.chart.Dashboard" table="DASHBOARD">
      <id name="dashboardId" column="DASHBOARDID" type="string"/>
      <property name="createdby" column="CREATEDBY" type="string"/>
      <property name="dashboardName" column="DASHBOARDNAME" type="string"/>
      <property name="dashboardType" column="DASHBOARDTYPE" type="string"/>
      <property name="dateCreated" column="DATECREATED" type="string"/>
      <property name="lastModifiedDate" column="LASTMODIFIEDDATE" type="string"/>
  </class>
</hibernate-mapping>

