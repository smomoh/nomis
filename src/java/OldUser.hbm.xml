<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class lazy="false" name="com.fhi.kidmap.business.OldUser" table="users">
    <id column="username" name="username" type="string">
      <!--<generator class="increment"/>-->
    </id>
    <property column="password" name="password" type="string"/>
    <property column="userrole_id" name="userroleId" type="string"/>
    <property column="first_name" name="firstName" type="string"/>
    <property column="last_name" name="lastName" type="string"/>
    <property column="state" name="state" type="string"/>
    <property column="CHANGEPWD" name="changePwd" type="string"/>
    <property column="VIEWCLIENTDETAILS" name="viewClientDetails" type="string"/>
  </class>
</hibernate-mapping>